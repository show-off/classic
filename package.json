{
  "name": "show-off",
  "version": "0.1.0",
  "private": true,
  "description": "Beautiful-as-fuck website portfolio templates",
  "keywords": [
    "portfolio",
    "template",
    "website",
    "beautiful"
  ],
  "bugs": {
    "url": "https://github.com/sidvenu/show-off/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidvenu/show-off"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Siddharth Venu <siddharthvvenu@gmail.com>",
  "scripts": {
    "build": "npm run format && gatsby build",
    "clean": "gatsby clean",
    "deploy": "./deploy.sh",
    "develop": "gatsby develop",
    "format": "npm run sortJson && prettier --write \"**/*.{css,ts,tsx,js,jsx,json}\"",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "serve": "gatsby serve",
    "sortJson": "npx sort-package-json && npx sort-package-json tsconfig.json",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^4.25.7",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.3",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1"
  }
}
